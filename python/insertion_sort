def insertion_sort(alist):
    for i in range(1, len(alist)):
        j = i-1
        next_el = alist[i]
        # Compare the current element with next one
        while (alist[j] > next_el) and (j >= 0):
            alist[j+1] = alist[j]
            j=j-1
        alist[j+1] = next_el

list = [19,2,31,45,30,11,121,27]
insertion_sort(list)
print(list)
