def bubblesort(list):
  # Swap the elements to arrange in order
   for up_limit in range(len(list)-1,0,-1):
      for i in range(up_limit):
         if list[i] > list[i+1]:
            temp = list[i]
            list[i] = list[i+1]
            list[i+1] = temp
list = [19,2,31,45,6,11,121,27]
bubblesort(list)
print(list)
